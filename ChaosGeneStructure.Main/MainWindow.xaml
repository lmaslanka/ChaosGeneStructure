<Window x:Class="ChaosGeneStructure.Main.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChaosGeneStructure.Main"
        mc:Ignorable="d"
        Title="MainWindow" Height="768" Width="1024">
    <DockPanel LastChildFill="True">
        <StackPanel Width="150" Background="AntiqueWhite">
            <Button x:Name="LoadData" Margin="10,10,10,0" Click="LoadData_Click">Load Data...</Button>
            <Button x:Name="Process" Margin="10,10,10,0" Click="Process_Click">Process...</Button>
            <TextBlock x:Name="ProcessingText" HorizontalAlignment="Center" Margin="10,40,10,0" FontSize="21">Processing...</TextBlock>
        </StackPanel>
        <DockPanel>
            <Canvas x:Name="Board"></Canvas>
        </DockPanel>
        
    </DockPanel>
</Window>
